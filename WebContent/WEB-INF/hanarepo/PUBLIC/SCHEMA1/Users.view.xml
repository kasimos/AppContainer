<mvc:View
	controllerName="ui5app.Users"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Table
		headerText="List all database users"
		id="usertable"
		items="{
			path: '/TABLE'
		}">
		<columns>
			<Column>
				<Text text="Name"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{USER_NAME}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</mvc:View>