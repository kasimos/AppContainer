<mvc:View height="100%" class="sapUiSizeCompact"
	controllerName="io.rtdi.hanaappcontainer.Controller"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	>

	<Panel height="100%">
		<headerToolbar>
			<OverflowToolbar>
				<Text text="All available apps" />
			</OverflowToolbar>
		</headerToolbar>
		<HBox id="idAppList"
			wrap="Wrap"
			items="{/apps}"
			width="100%"
			height="100%">
			<core:Fragment fragmentName="io.rtdi.hanaappcontainer.ui5libs.fragments.applicationtile" type="XML" />
		</HBox>
	</Panel>
</mvc:View>

