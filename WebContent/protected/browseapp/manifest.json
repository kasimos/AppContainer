{
	"sap.app": {
		"id": "io.rtdi.hanaappcontainer.browseapp",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"ObjectOwner": {
				"uri": "../../../../odata/SYS/SCHEMAS/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "io.rtdi.hanaappcontainer.browseapp.View",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"libs": {
				"sap.m": {
					
				}
			}
		},
		"config": {
			"sample": {
				"stretch": true,
				"files": [
					"Component.js",
					"Controller.controller.js",
					"View.view.xml",
					"manifest.json"
				]
			}
		},
		"models": {
			"ObjectOwner": {
				"preload": true,
				"dataSource": "ObjectOwner",
				"settings": {
					"autoExpandSelect": true,
					"operationMode": "Server",
					"groupId": "$direct",
					"synchronizationMode": "None"
				}
			}
		}
	}
}