{
	"dataSource": {
		"url": "ui5rest/query?$select=SELECT OBJECT_TYPE, count(*) as COUNT FROM objects GROUP BY OBJECT_TYPE order by 2 desc"
	},
	"xAxes": [
		{
			"type": "CategoryAxis",
			"dataFields": {
				"category": "OBJECT_TYPE"
			}
		}
	],
	"yAxes": [
		{
			"type": "ValueAxis"
		}
	],
	"series": [
		{
			"type": "RadarSeries",
			"name": "Number of Objects",
			"strokeWidth": 3,
			"zIndex": 2,
			"dataFields": {
				"valueY": "COUNT",
				"categoryX": "OBJECT_TYPE"
			}
		}
	]
}